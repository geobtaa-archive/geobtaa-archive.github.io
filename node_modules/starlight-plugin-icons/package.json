{
  "name": "starlight-plugin-icons",
  "type": "module",
  "version": "1.1.3",
  "packageManager": "bun@1.2.21",
  "description": "Add icons to Astro Starlight: sidebar, codeblocks, file tree",
  "license": "MIT",
  "homepage": "https://github.com/Rettend/starlight-plugin-icons#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rettend/starlight-plugin-icons.git"
  },
  "bugs": {
    "url": "https://github.com/Rettend/starlight-plugin-icons/issues"
  },
  "keywords": [
    "astro",
    "icons",
    "starlight",
    "astro-integration"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./uno": {
      "types": "./src/uno.ts",
      "import": "./src/uno.ts"
    },
    "./styles/*": "./src/styles/*",
    "./components/*": "./src/components/*"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "prepublishOnly": "node -e \"require('fs').copyFileSync('../../README.md', 'README.md')\"",
    "postpublish": "node -e \"require('fs').unlinkSync('README.md')\"",
    "lint": "eslint . --fix",
    "update:deps": "taze latest -wr",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "@astrojs/starlight": ">=0.35.0",
    "astro": ">=4",
    "typescript": "^5",
    "unocss": ">=0.58.0",
    "zod": ">=3.22.0 || >=4.0.0"
  },
  "dependencies": {
    "glob": "^11.0.3",
    "hast-util-select": "^6.0.4",
    "hast-util-to-string": "^3.0.1",
    "hastscript": "^9.0.1",
    "rehype": "^13.0.2",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@types/bun": "latest",
    "@typescript/native-preview": "^7.0.0-dev.20250910.1",
    "@unocss/eslint-plugin": "^66.5.1",
    "astro-eslint-parser": "^1.2.2",
    "bumpp": "^10.2.3",
    "eslint": "^9.35.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-format": "^1.0.1",
    "prettier-plugin-astro": "^0.14.1",
    "taze": "^19.6.0"
  }
}
